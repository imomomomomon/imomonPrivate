package bit.basic.obj2;

public class ObjectEx2 {
	private String msg;
	public ObjectEx2() {
		msg = "hi";
	}
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}
	public static void main(String[] args) {
		ObjectEx2 o = new ObjectEx2();
		System.out.println(o.getMsg());
	}
}
