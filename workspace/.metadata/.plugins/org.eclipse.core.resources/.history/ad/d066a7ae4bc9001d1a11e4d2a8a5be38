package bit.basic.obj1;

public class Member {
}

class MemberInfo {
	private String name;
	private String id;
	private String email;
	private int countValue;
	
	public MemberInfo(String name, String id, String email, int countValue) {
		super();
		this.name = name;
		this.id = id;
		this.email = email;
		this.countValue = countValue;
	}
}
