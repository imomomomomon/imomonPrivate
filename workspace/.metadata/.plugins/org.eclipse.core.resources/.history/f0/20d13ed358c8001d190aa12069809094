package bit.basic.array;

public class ArrayEx1 {
	
	public void arrayMethod() {
		int[] m = new int[5];
		
		for (int i = 0; i < m.length; i++) {
			m[i] = (int)(Math.random()*100);
		}
		
		System.out.println(m[i]);

	}
	public static void main(String[] args) {
		ArrayEx1 a = new ArrayEx1();
		a.arrayMethod();
	}
}
